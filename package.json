{
    "name": "home-stuff",
    "version": "0.0.0",
    "description": "",
    "author": "ikruchkov0@gmail.com",
    "scripts": {
        "dev": "vite --port 3000",
        "build": "vite build",
        "serve": "vite preview --port 5000",
        "test": "vitest run",
        "vitest": "vitest watch --ui",
        "coverage:run": "vitest run --coverage",
        "coverage:badge": "node ./build/coverage-badge-updater.mjs",
        "coverage": "pnpm coverage:run && pnpm coverage:badge",
        "prepare": "husky install",
        "favicons": "node ./build/favicons.mjs",
        "ts:check": "tsc --noEmit",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint -c ./.eslintrc.js ./src/  ./e2e/",
        "e2e": "pnpm e2e:test && pnpm e2e:coverage",
        "e2e:test": "playwright test --project chromium",
        "e2e:coverage": "pnpm e2e:coverage-merge && pnpm e2e:coverage-report",
        "e2e:coverage-merge": "nyc merge ./.artifacts/e2e/raw ./.artifacts/e2e/merged/merged.json",
        "e2e:coverage-report": "nyc report --temp-dir ./.artifacts/e2e/merged --report-dir ./.artifacts/e2e/coverage  --reporter=html --reporter=json-summary"
    },
    "license": "MIT",
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^5.0.0",
        "@iconify-json/mdi": "^1.1.50",
        "@playwright/test": "^1.32.0",
        "@solid-primitives/i18n": "^1.2.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@types/jsdom": "^21.1.1",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@vitest/coverage-c8": "^0.29.7",
        "@vitest/ui": "^0.29.7",
        "autoprefixer": "^10.4.14",
        "cssnano": "^5.1.15",
        "daisyui": "^2.51.5",
        "eslint": "^8.36.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-css": "^0.7.0",
        "eslint-plugin-i18next": "6.0.0-7",
        "eslint-plugin-solid": "^0.11.0",
        "eslint-plugin-tailwindcss": "^3.10.1",
        "favicons": "^7.1.1",
        "husky": "^8.0.3",
        "jsdom": "^21.1.1",
        "jwt-decode": "^3.1.2",
        "node-html-parser": "^6.1.5",
        "nyc": "^15.1.0",
        "postcss": "^8.4.21",
        "prettier": "^2.8.6",
        "rollup-plugin-visualizer": "^5.9.0",
        "solid-testing-library": "^0.5.1",
        "tailwindcss": "^3.2.7",
        "typescript": "^5.0.2",
        "unplugin-icons": "^0.15.3",
        "v8-to-istanbul": "^9.1.0",
        "vite": "^4.2.1",
        "vite-plugin-solid": "^2.6.1",
        "vite-tsconfig-paths": "^4.0.7",
        "vitest": "^0.29.7"
    },
    "dependencies": {
        "solid-js": "^1.6.15"
    }
}

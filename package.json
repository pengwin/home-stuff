{
    "name": "home-stuff",
    "version": "0.0.0",
    "description": "",
    "author": "ikruchkov0@gmail.com",
    "scripts": {
        "dev": "vite --port 3000",
        "build": "vite build",
        "serve": "vite preview --port 5000",
        "test": "vitest run",
        "coverage:run": "vitest run --coverage",
        "coverage:badge": "coverage-badge-creator --config ./configs/badge-coverage/.badge-config",
        "coverage:e2e-badge": "coverage-badge-creator --config ./configs/badge-coverage/.e2e-badge-config",
        "coverage": "pnpm coverage:run && pnpm coverage:badge",
        "prepare": "husky install",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint src/* ",
        "e2e": "pnpm e2e:test && pnpm e2e:coverage-report && pnpm coverage:e2e-badge",
        "e2e:test": "playwright test --project chromium",
        "e2e:coverage-html": "istanbul report --include ./.artifacts/e2e/raw/coverage_*.json --dir ./.artifacts/e2e/coverage/ html",
        "e2e:coverage-report": "istanbul report --include ./.artifacts/e2e/raw/coverage_*.json --dir ./.artifacts/e2e/coverage json-summary "
    },
    "license": "MIT",
    "devDependencies": {
        "@playwright/test": "^1.28.1",
        "@solid-primitives/i18n": "^1.1.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@types/jsdom": "^20.0.1",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "autoprefixer": "^10.4.13",
        "c8": "^7.12.0",
        "coverage-badge-creator": "^1.0.14",
        "cssnano": "^5.1.14",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-css": "^0.6.0",
        "eslint-plugin-solid": "^0.7.4",
        "eslint-plugin-tailwindcss": "^3.7.0",
        "husky": "^8.0.2",
        "istanbul": "^0.4.5",
        "jsdom": "^20.0.3",
        "postcss": "^8.4.19",
        "prettier": "^2.8.0",
        "rollup-plugin-visualizer": "^5.8.3",
        "solid-testing-library": "^0.3.0",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.3",
        "v8-to-istanbul": "^9.0.1",
        "vite": "^3.2.4",
        "vite-plugin-solid": "^2.4.0",
        "vitest": "^0.17.1"
    },
    "dependencies": {
        "solid-js": "^1.6.2"
    }
}

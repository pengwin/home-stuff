{
    "name": "home-stuff",
    "version": "0.0.0",
    "description": "",
    "author": "ikruchkov0@gmail.com",
    "scripts": {
        "dev": "vite --port 3000",
        "build": "vite build",
        "serve": "vite preview --port 5000",
        "test": "vitest run",
        "coverage:run": "vitest run --coverage",
        "coverage:badge": "node ./build/coverage-badge-updater.mjs",
        "coverage:e2e-badge": "coverage-badge-creator --config ./configs/badge-coverage/.e2e-badge-config",
        "coverage": "pnpm coverage:run && pnpm coverage:badge",
        "prepare": "husky install",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint -c ./.eslintrc.js ./src/  ./e2e/",
        "e2e": "pnpm e2e:test && pnpm e2e:coverage",
        "e2e:test": "playwright test --project chromium",
        "e2e:coverage": "pnpm e2e:coverage-merge && pnpm e2e:coverage-report",
        "e2e:coverage-merge": "nyc merge ./.artifacts/e2e/raw ./.artifacts/e2e/merged/merged.json",
        "e2e:coverage-report": "nyc report --temp-dir ./.artifacts/e2e/merged --report-dir ./.artifacts/e2e/coverage  --reporter=html --reporter=json-summary"
    },
    "license": "MIT",
    "devDependencies": {
        "@fullhuman/postcss-purgecss": "^5.0.0",
        "@iconify-json/mdi": "^1.1.36",
        "@playwright/test": "^1.28.1",
        "@solid-primitives/i18n": "^1.1.2",
        "@storeon/solidjs": "^3.0.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@types/jsdom": "^20.0.1",
        "@types/testing-library__jest-dom": "^5.14.5",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vitest/coverage-c8": "^0.25.3",
        "autoprefixer": "^10.4.13",
        "cssnano": "^5.1.14",
        "daisyui": "^2.42.1",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-css": "^0.6.0",
        "eslint-plugin-solid": "^0.9.1",
        "eslint-plugin-tailwindcss": "^3.7.1",
        "husky": "^8.0.2",
        "jsdom": "^20.0.3",
        "nyc": "^15.1.0",
        "postcss": "^8.4.19",
        "prettier": "^2.8.0",
        "rollup-plugin-visualizer": "^5.8.3",
        "solid-testing-library": "^0.5.0",
        "storeon": "^3.1.5",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.3",
        "unplugin-icons": "^0.14.14",
        "v8-to-istanbul": "^9.0.1",
        "vite": "^3.2.4",
        "vite-plugin-solid": "^2.4.0",
        "vite-tsconfig-paths": "^3.6.0",
        "vitest": "^0.25.3"
    },
    "dependencies": {
        "solid-js": "^1.6.2"
    }
}
